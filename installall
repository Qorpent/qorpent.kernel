<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<Import Condition="Exists('..\qorpent.sys')" Project="..\qorpent.sys\commons\install.targets" />	
	<Import Condition="Exists('qorpent.sys')" Project="qorpent.sys\commons\install.targets" />	
	<ItemGroup>
		<Project Condition="Exists('..\qorpent.sys')" Include="..\qorpent.sys\install" />
		<Project Condition="Exists('qorpent.sys')" Include=".\qorpent.sys\install" />
		<Project Condition="Exists('..\qorpent.sys')" Include="..\*\install" Exclude="..\qorpent.sys\install" />
		<Project Condition="Exists('qorpent.sys')" Include=".\*\install" Exclude="..\qorpent.sys\install" />
	</ItemGroup>
	<Target Name="Build">
		<MSBuild Projects="@(Project)" Properties="NoProjRef=True" />
	</Target>
</Project>
<?xml version="1.0" encoding="UTF-8"?>
<!-- this is stub and very basic web-config, which does not contains anything valueable,
will be rewrited with qorpent.sys web.config or another file -->
<configuration>
	<system.web>
		<trust level="Full" />
		<httpRuntime maxRequestLength="10240" />
	</system.web>
	<system.webServer>
		<httpErrors errorMode="Detailed" />
		<staticContent>
			<mimeMap fileExtension=".less" mimeType="text/css" />
		</staticContent>
		<validation validateIntegratedModeConfiguration="false" />
		<security>
			<requestFiltering>
				<requestLimits maxQueryString="2147483647" maxAllowedContentLength="2147483647" />
				<fileExtensions>
					<add fileExtension=".less" allowed="true" />
				</fileExtensions>
			</requestFiltering>
		</security>
		
	</system.webServer>

</configuration>
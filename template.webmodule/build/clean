#!/bin/sh
echo start clean
rm -rf .output
rm -rf .tmp
rm -rf ./css/*.css
rm -rf ./css/**/*.css
rm -rf ./css/*.map
rm -rf ./css/**/*.map
rm -rf ./css-src/*.css
rm -rf ./css-src/**/*.css
rm -rf npm-debug.log
rm -rf js/*.js
rm -rf js/*.map
rm -rf js-src/*_api.js
rm -rf js-src/*_types.js
rm -rf cs/*
rm -rf bxls/.output
for f in js-src/*.ts js-src/**/*.ts; do
    if echo $f | grep -E '^[^\\*]+$' > /dev/null; then
        rm -rf ${f%%.*}.js*
    fi
done
echo clean finished
